def numeros_divisiveis():
    # Solicita ao usuário o valor do divisor
    divisor = int(input("Entre com o valor do divisor: "))
    # Solicita ao usuário o valor inicial do intervalo
    inicio = int(input("Início do intervalo: "))
    # Solicita ao usuário o valor final do intervalo
    fim = int(input("Final do intervalo: "))

    print(f"Números divisíveis por {divisor} no intervalo de {inicio} a {fim}:")
    # Loop pelo intervalo especificado
    for numero in range(inicio, fim + 1):
        # Verifica se o número é divisível pelo divisor
        if numero % divisor == 0:
            print(numero, end=" ")

numeros_divisiveis()
